@using RefilWeb.Models
@model IEnumerable<RefilWeb.Models.User>
@{
    ViewBag.Title = "AdminPromote";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="page-header">
            <h2>Promote</h2>
        </div>
        
        @foreach (var user in Model)
        {
            <a href="/admin/promote/@user.UserId">promote @user.FullName</a>
            <br />
        }

    </div>
</div>


